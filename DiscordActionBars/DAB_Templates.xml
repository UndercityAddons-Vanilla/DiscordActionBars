		<OnEvent>
			DAB_OtherBar_OnEvent(event);
		</OnEvent>
		<OnUpdate>
			DAB_OtherBar_OnUpdate(arg1);
		</OnUpdate>
		<OnHide>
			if (this.moving) then
				DAB_OtherBar_OnDragStart();
			end
		</OnHide>
		<OnLeave>
			if (this.moving) then
				DAB_OtherBar_OnDragStart();
