					<Anchor point="RIGHT" relativePoint="LEFT">
						<Offset><AbsDimension x="-5" y="0"/></Offset>
					</Anchor>
				</Anchors>
			</FontString>
		</Layer>
	</Layers>
</Frame>

<Button name="DL_NudgeTemplate" inherits="UIPanelButtonTemplate" virtual="true">
