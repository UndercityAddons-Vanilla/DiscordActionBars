		<BackgroundInsets>
			<AbsInset left="10" right="5" top="4" bottom="9"/>
		</BackgroundInsets>
		<TileSize>
			<AbsValue val="16"/>
		</TileSize>
		<EdgeSize>
			<AbsValue val="16"/>
		</EdgeSize>
